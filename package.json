{
  "name": "SecurityScout",
  "productName": "SecurityScout",
  "author": "Fabian Frischmann",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "electron:build:publish": "cross-env GH_TOKEN=4bd5080e445604c57c5ecff88986ac71e427c6e3 && npm run build:prod && electron-builder build --publish=always",
    "electron:build:all": "npm run build:prod && electron-builder build --publish=never -mwl",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "dependencies": {
    "@akveo/ng2-completer": "^9.0.1",
    "@angular/animations": "^11.0.9",
    "@angular/cdk": "11.0.4",
    "@angular/common": "^11.0.9",
    "@angular/compiler": "^11.0.9",
    "@angular/core": "^11.0.9",
    "@angular/forms": "^11.0.9",
    "@angular/google-maps": "^11.0.4",
    "@angular/material": "^11.0.4",
    "@angular/platform-browser": "^11.0.9",
    "@angular/platform-browser-dynamic": "^11.0.9",
    "@angular/router": "^11.0.9",
    "@asymmetrik/ngx-leaflet": "3.0.1",
    "@nebular/auth": "7.0.0",
    "@nebular/eva-icons": "7.0.0",
    "@nebular/security": "7.0.0",
    "@nebular/theme": "7.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@swimlane/ngx-charts": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "angular2-chartjs": "0.4.1",
    "bootstrap": "4.6.0",
    "chai": "^4.3.0",
    "chart.js": "2.9.4",
    "ckeditor": "4.7.3",
    "classlist.js": "1.1.20150312",
    "core-js": "^2.5.1",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "echarts": "^4.0.2",
    "electron-log": "^4.3.1",
    "electron-updater": "^4.3.5",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eva-icons": "^1.1.3",
    "fs": "0.0.1-security",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "karma-electron": "^6.3.3",
    "leaflet": "1.7.1",
    "nebular-icons": "1.1.0",
    "ng2-ckeditor": "^1.2.9",
    "ng2-smart-table": "^1.6.0",
    "ngx-echarts": "^4.2.2",
    "node-sass": "^4.14.1",
    "normalize.css": "6.0.0",
    "pace-js": "1.0.2",
    "roboto-fontface": "0.8.0",
    "rxjs": "6.6.2",
    "rxjs-compat": "6.3.0",
    "socicon": "3.0.5",
    "spectron": "^13.0.0",
    "style-loader": "^1.1.3",
    "timers": "^0.1.1",
    "tinymce": "4.5.7",
    "tslib": "^2.1.0",
    "typeface-exo": "0.0.22",
    "wait-on": "^5.2.1",
    "web-animations-js": "^2.3.2",
    "webdriver-manager": "^12.1.8",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "^11.0.7",
    "@angular/compiler-cli": "^11.0.9",
    "@angular/language-service": "11.0.9",
    "@compodoc/compodoc": "1.1.11",
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@types/d3-color": "1.0.5",
    "@types/googlemaps": "^3.39.3",
    "@types/leaflet": "1.2.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "conventional-changelog-cli": "^1.3.4",
    "electron": "^11.2.3",
    "husky": "0.13.3",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "npm-run-all": "^4.0.2",
    "protractor": "~7.0.0",
    "rimraf": "2.6.1",
    "electron-builder": "^22.9.1",
    "electron-reload": "^1.5.0",
    "electron-serve": "^1.1.0",
    "stylelint": "7.13.0",
    "ts-node": "^3.2.2",
    "tslint": "~6.1.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^4.0.5",
    "@types/mocha": "^8.2.1",
    "mocha": "^8.3.0"
  },
  "build": {
    "appId": "SecurityScout",
    "asar": true,
    "files": [
      "**/*",
      "!**/*.ts",
      "!*.code-workspace",
      "!LICENSE.md",
      "!package.json",
      "!package-lock.json",
      "!src/",
      "!e2e/",
      "!hooks/",
      "!angular.json",
      "!_config.yml",
      "!karma.conf.js",
      "!tsconfig.json",
      "!tslint.json"
    ],
    "directories": {
      "output": "release/"
    },
    "win": {
      "icon": "dist/assets/images/desktop-logo.png",
      "target": [
        "portable",
        "nsis"
      ]
    },
    "nsis": {
      "installerIcon": "dist/assets/images/desktop-logo.ico",
      "uninstallerIcon": "dist/assets/images/desktop-logo.ico",
      "uninstallDisplayName": "SecurityScout",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "warningsAsErrors": false,
      "installerSidebar": "dist/assets/images/installer-background.bmp",
      "uninstallerSidebar": "dist/assets/images/installer-background.bmp"
    }
  }
}
